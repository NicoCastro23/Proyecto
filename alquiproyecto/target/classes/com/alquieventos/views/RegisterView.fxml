<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:controller="com.alquieventos.controllers.RegisterController"
            xmlns:fx="http://javafx.com/fxml"
            prefHeight="600.0" prefWidth="600.0">
    <TextField fx:id="nameField" promptText="Nombre Completo" layoutX="200.0" layoutY="100.0"/>
    <TextField fx:id="emailField" promptText="Email" layoutX="200.0" layoutY="140.0"/>
    <TextField fx:id="idField" promptText="ID" layoutX="200.0" layoutY="180.0"/>
    <TextField fx:id="phoneField" promptText="Phone Number" layoutX="200.0" layoutY="220.0"/>
    <PasswordField fx:id="passwordField" promptText="Password" layoutX="200.0" layoutY="260.0"/>
    <Button text="Registrarse" onAction="#handleRegister" layoutX="260.0" layoutY="300.0"/>
</AnchorPane>

